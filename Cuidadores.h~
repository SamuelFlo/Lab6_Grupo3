#include <iostream>
#include <string>
#include <vector>

using namespace std;

#ifndef CUIDADORES_H
#define CUIDADORES_H
class Cuidadores{
	protected:
		string nombre;
		string id;
		string sexo;
		string Edad;
	public:
		Cuidadores(string,string,string,string);
		Cuidadores();
		string getNombre();
		void setNombre(string);

		string getId();
		void setId(string);

		string getSexo();
		void setSexo(string);

		string getEdad();
		void setEdad(string);

};
#endif